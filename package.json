{
    "name": "bigscreener",
    "version": "1.3.0",
    "private": true,
    "description": "A free and high quality streaming service using WebTorrent",
    "license": "UNLICENSED",
    "repository": {
        "url": "https://github.com/yunseok/BigScreener"
    },
    "author": {
        "name": "yunseok",
        "email": "yunseok.noreply@gmail.com"
    },
    "main": "main.js",
    "build": {
        "appId": "com.bigscreener.app",
        "productName": "BigScreener",
        "directories": {
            "output": "./build"
        },
        "win": {
            "target": [
                "nsis"
            ],
            "icon": "./assets/imgs/icon.ico"
        },
        "nsis": {
            "deleteAppDataOnUninstall": true,
            "createDesktopShortcut": true,
            "createStartMenuShortcut": true,
            "shortcutName": "BigScreener",
            "artifactName": "bigscreener.setup.${version}.${ext}"
        },
        "linux": {
            "target": [
                "deb"
            ],
            "icon": "./assets/imgs/icon.png",
            "category": "video"
        }
    },
    "scripts": {
        "start": "babel ./src --out-dir dist/ && electron .",
        "build": "babel ./src --out-dir dist/",
        "pack": "electron-builder"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "babel-register": "^6.26.0",
        "cheerio": "^1.0.0-rc.3",
        "decompress": "^4.2.0",
        "download": "^7.1.0",
        "electron-json-storage": "^4.1.6",
        "electron-squirrel-startup": "^1.0.0",
        "electron-titlebar": "0.0.3",
        "electron-window-state": "^5.0.3",
        "fast-average-color": "^5.1.0",
        "firebase": "^6.0.2",
        "franc-min": "^4.1.0",
        "fs-extra": "^8.1.0",
        "get-json": "^1.0.1",
        "https-proxy-agent": "^2.2.1",
        "langs": "^2.0.0",
        "parse-torrent-name": "^0.5.4",
        "pluralize": "^8.0.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-lazy-load": "^3.0.13",
        "react-reveal": "^1.2.2",
        "react-transition-group": "^1.2.1",
        "torrent-search-api": "^2.0.8",
        "uniqid": "^4.1.1",
        "utf8": "^3.0.0",
        "webtorrent": "^0.103.1",
        "webtorrent-health": "^1.1.2"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "electron": "^5.0.2",
        "electron-builder": "^21.0.15"
    }
}
